<Application x:Class="Alphicsh.MusicRoom.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Alphicsh.MusicRoom"
             xmlns:converters="clr-namespace:Alphicsh.MusicRoom.Converters">
    <Application.Resources>
        <converters:SwitchConverter x:Key="SwitchConverter"/>
        <converters:NumericWithDefaultConverter x:Key="NumericConverter"/>
        <Style TargetType="{x:Type TextBox}">
            <EventSetter Event="KeyDown" Handler="TextBox_KeyDown"/>
        </Style>
    </Application.Resources>
</Application>
